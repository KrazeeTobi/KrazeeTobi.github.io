;
; Hardware Install Script for INTSYS Processor Device
;

[Version]
Signature=$CHICAGO$
Class=Processor
Provider=%INTSYS%
SetupClass=BASE

;================================
[ClassInstall]
CopyFiles=CopyFiles.INF
AddReg=AddRegClass

[AddRegClass]
HKR,,,,%Processor%

;================================
[Manufacturer]
%INTSYS%=INTSYS

[INTSYS]
%E64.DeviceDesc%=E64,SCSI\INTSYS__NUS_ROM_EMULATOR0
%CGB.DeviceDesc%=CGB,SCSI\INTSYS__IS-CGB-EMULATOR_0
%ISD.DeviceDesc%=ISD,SCSI\INTSYS__SHVC_DEBUGGER___0
%ISC.DeviceDesc%=ISC,SCSI\INTSYS__SHVC_CHARACTER__0
%ISF.DeviceDesc%=ISF,SCSI\INTSYS__Flash_Writer____0

[E64]
CopyFiles=CopyFiles.VXD
AddReg=AddReg.VXD

[CGB]
CopyFiles=CopyFiles.VXD
AddReg=AddReg.VXD

[ISD]
CopyFiles=CopyFiles.VXD
AddReg=AddReg.VXD

[ISC]
CopyFiles=CopyFiles.VXD
AddReg=AddReg.VXD

[ISF]
CopyFiles=CopyFiles.VXD
AddReg=AddReg.VXD

[INTSYS__NUS_ROM_EMULATOR.Det]
CopyFiles=CopyFiles.VXD
AddReg=AddReg.VXD

[INTSYS__IS-CGB-EMULATOR_.Det]
CopyFiles=CopyFiles.VXD
AddReg=AddReg.VXD

[INTSYS__SHVC_DEBUGGER___.Det]
CopyFiles=CopyFiles.VXD
AddReg=AddReg.VXD

[INTSYS__SHVC_CHARACTER__.Det]
CopyFiles=CopyFiles.VXD
AddReg=AddReg.VXD

[INTSYS__Flash_Writer____.Det]
CopyFiles=CopyFiles.VXD
AddReg=AddReg.VXD

[CopyFiles.VXD]
ISPD.VXD

[CopyFiles.INF]
ISPD.INF

[DestinationDirs]
CopyFiles.VXD=12
CopyFiles.INF=17

[SourceDisksFiles]
ISPD.INF=1
ISPD.VXD=1

[SourceDisksNames]
1 = "INTSYS Processor Device Installation Disk", ISD, 0000-0000

[AddReg.VXD]
HKR,,DeviceDriver,,ISPD.VXD
HKR,,Class,,Processor

;================================
[Strings]
INTSYS="INTELLIGENT SYSTEMS Co., Ltd."
Processor="Processor Device"
E64.DeviceDesc="INTSYS NUS ROM EMULATOR"
CGB.DeviceDesc="IS-CGB-EMULATOR"
